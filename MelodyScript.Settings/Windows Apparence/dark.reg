Windows Registry Editor Version 5.00

; sets black color and enables color accent on title bars

[HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\DWM]
"ColorizationColor"=dword:ff2b2b2b
"ColorizationAfterglow"=dword:ff2b2b2b
"AccentColor"=dword:ff000000
"ColorPrevalence"=dword:00000001
"AccentColorInactive"=dword:ff2b2b2b

[HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\Accent]
"AccentColorMenu"=dword:ff2b2b2b